<!DOCTYPE html>
<html>
  <head>
    <title>JobbyBoy</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link href="https://fonts.googleapis.com/css?family=Noto+Sans|Open+Sans|Quicksand:300,400,500,600,700|Raleway|Roboto|Rubik&display=swap" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"
            integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo="
            crossorigin="anonymous">
    </script>

    <script type="text/javascript">
      window.icon1 = "<%= image_url('resume-icon.png') %>";
      window.icon2 = "<%= image_url('swatch-icon.png') %>";
      window.icon3 = "<%= image_url('api-icon.png') %>";
    </script>

  </head>

  <body>
    <%= yield %>
  </body>
</html>
